<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DepartmentStoreActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".EvaluateActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".GoodsOrderActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".EvaluateDetailActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".EvaluateGoodsActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".EvaluatePhotoActivity" android:theme="@style/AppCompatTheme" />
    </application>

</manifest>